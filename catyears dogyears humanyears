var humanYearsCatYearsDogYears = function(humanYears) {
let a = []
  if (humanYears==1){
  return a= [1,15,15];
}else if (humanYears==2){
  return a= [2,24,24];
}else if (humanYears==3){
  return a= [3,28,29];
}else if (humanYears>3){
  return a=[humanYears,28+(4*(humanYears-3)),29+(5*(humanYears-3))]
  }


  return a = [0,0,0];
}
